
<div id="media" class="container-fluid height">
	<div class="col-lg-8 col-md-8 col-sm-12 col-xs-12 no-gutters insta">
		<div class="row background">
			<div class="row no-gutters title">
				
					<a href="https://www.instagram.com/velveteenensemble/" target="_blank"><h2><%=image_tag image_url("https://s3.amazonaws.com/velveteenensemble/blinsta.png")%>Instagram</h2></a>
			</div>

				<script>
	  			var feed = new Instafeed({
						target: "instafeed",
		        get: 'user',
		        userId: '<%= ENV['USER_ID'] %>',
		        accessToken: '<%= ENV['ACCESS_TOKEN'] %>',
		        clientId: '<%= ENV['CLIENT_ID'] %>',
		        sortBy: 'most-recent',
		        limit: 20,
		        resolution: 'standard_resolution',
		        template: '<a href="{{link}}" target="_blank"><div class="img" style="background-image:url({{image}})"></div></a>',
		        links: true,
	        mock: false,
	        useHttp: false
		    	});
				  feed.run();

				</script>

	    <div class="col-md-12" id="instafeed"></div>
	  </div>
	</div>
	<div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 no-gutters">
		<div class="row background">
  		<div class="row no-gutters title">
				<a href="https://www.youtube.com/channel/UCtaLy-ddOLTPuIyXznzzu2A" target="_blank"><h2><%=image_tag image_url("https://s3.amazonaws.com/velveteenensemble/blyt2.png")%>Youtube</h2></a>
			</div>
			<div class="col-lg-6 col-md-12 col-sm-4 col-xs-12 no-gutters youtube">
		  	<iframe width="90%" height="auto" src="http://www.youtube.com/embed?max-results=1&controls=0&showinfo=0&rel=0&listType=user_uploads&list=marcellione" frameborder="10px" allowfullscreen></iframe> 
		  </div>
		</div>
	</div>
</div>